{
  "env": {
    "test": {
      "presets": [["next/babel", { "preset-env": { "modules": "commonjs" } }]]
    }
  },
  "presets": ["next/babel"],
  "plugins": [
    [
      "transform-define", "./env-config.js"
    ],
    [
      "module-resolver", {
        "root": ["."],
        "alias": {
          "styles": "./styles"
        },
        "cwd": "babelrc"
    }],
    [
      "wrap-in-js", {
        "extensions": ["css$", "scss$"]
      }
    ]
  ],
  "ignore": []
}
